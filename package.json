{
  "name": "e2r5header",
  "displayName": "e2r5 Header",
  "description": "e2r5 header for VSCode",
  "icon": "images/E2R5.png",
  "version": "0.42.9",
  "publisher": "Glouyot",
  "repository": {
    "type": "git",
    "url": "https://github.com/E2R5-corp/VS-code-Header"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "e2r5header.username": {
          "type": "string",
          "description": "Username to use in e2r5 headers"
        },
        "e2r5header.email": {
          "type": "string",
          "description": "Email address to use in e2r5 headers"
        },
        "e2r5header.headerOnSave": {
          "type": "boolean",
          "description": "Automatically append header on save",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "e2r5header.insertHeader",
        "title": "Insert e2r5 header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "e2r5header.insertHeader",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^6.14.7",
    "typescript": "^2.9.2",
    "vscode": "^1.0.0"
  },
  "dependencies": {}
}
